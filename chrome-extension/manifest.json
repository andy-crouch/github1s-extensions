{
  "name": "Github 1s",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension enables GitHub 1s on a GitHub repository Url so that the repository can be viewed with VS Code.",
  "homepage_url": "http://amcrou.ch",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "GitHub 1s Disabled"
  },
  "permissions": [
    "tabs"
  ]
}